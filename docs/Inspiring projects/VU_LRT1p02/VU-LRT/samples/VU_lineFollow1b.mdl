    type		    FUNC_STATE
    decomposition	    CLUSTER_STATE
    eml {
      isEML		      1
      script		      "function v= velocity (pulseCount,gainFactor)\n\n% Velocity computes the velocity in engineering u"
      "nits defined by gainFactor\n% If gainFactor=1, => units are pulses/sample.  There are 360 pulses per rev\n%\n% E"
      "xample:  To obtain velocity in rev/sec\n% velocity = (pulses/sample) * (samples/sec) * (rev/pulses) = rev/sec\n%"
      " Given a sample period T=0.1, and the known 360 pulses per rev...\n% => gainFactor = (1/0.1) * (1/360) = 1/36\n%"
      "\n% Not clear what happens when the pulse count overflows\n\n\n% Initialize persistent variables\npersistent pre"
      "vPulseCount\n\nif (isempty(prevPulseCount)),\n    prevPulseCount= int32(0); \nend\n\n% Compute the velocity in u"
      "nits defined by gainFactor\nv= double(pulseCount - prevPulseCount) * gainFactor;\nprevPulseCount= pulseCount;\n\n"
      "\n\n"
      editorLayout	      "100 M4x1[112 307 945 623]"
      fimathString	      "fimath(...\n'RoundMode', 'floor',...\n'OverflowMode', 'wrap',...\n'ProductMode', 'KeepLSB', "
      "'ProductWordLength', 32,...\n'SumMode', 'KeepLSB', 'SumWordLength', 32,...\n'CastBeforeSum', true)"
      fimathForFiConstructors FimathMatlabFactoryDefault
      emlDefaultFimath	      FimathUserSpecified
    }
  }
