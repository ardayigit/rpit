    eml {
      isEML		      1
      script		      "function [leftMotor, rightMotor] = fsm(clock, sensedLight)\n\npersistent currentState maxLight mi"
      "nLight threshold \n\nif isempty(currentState),\n    currentState= uint8(0);\n    maxLight= uint16(0);\n    minLi"
      "ght= uint16(1023);\n    threshold= uint16(45);\nend;\n\nleftMotor= 0;\nrightMotor= 0;\n\nswitch currentState\n  "
      "  case 0 %Calibration - turning left\n        leftMotor= -0.4;\n        rightMotor= 0.4;\n        if sensedLight"
      ">maxLight, maxLight= sensedLight; end;\n        if sensedLight<minLight, minLight= sensedLight; end;\n        if"
      " (clock>500), currentState= uint8(1); end;\n    case 1 %Calibration - turning right\n        leftMotor= 0.4;\n  "
      "      rightMotor= -0.4;\n        if sensedLight>maxLight, maxLight= sensedLight; end;\n        if sensedLight<mi"
      "nLight, minLight= sensedLight; end;\n        if (clock>1400), \n            threshold = (maxLight + minLight)/2;"
      "\n            currentState= uint8(2); \n        end;\n    case 2 %Too much light => turn Left\n        leftMotor"
      "= 0.4;\n        rightMotor= 1.0;\n        if sensedLight<threshold, currentState= uint8(3); end;\n    case 3 %To"
      "o little light => turn Right\n        leftMotor= 1.0;\n        rightMotor= 0.4;\n        if sensedLight>threshol"
      "d, currentState= uint8(2); end;\nend\n\n"
      editorLayout	      "100 M4x1[201 167 1080 733]"
    }
  }
