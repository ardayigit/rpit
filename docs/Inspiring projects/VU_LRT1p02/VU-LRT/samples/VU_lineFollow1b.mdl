  state {
    id			    25
    labelString		    "eML_blk_kernel()"
    position		    [18 64.5 118 66]
    fontSize		    12
    chart		    24
    treeNode		    [24 0 0 0]
    superState		    SUBCHART
    subviewer		    24
    ssIdNumber		    1
    type		    FUNC_STATE
    decomposition	    CLUSTER_STATE
    eml {
      isEML		      1
      script		      "function mv= PID (setPoint,contolledVariable,Kc,Ti,Td,mvMax,mvMin)\n\n% PID uses the 'velocity al"
      "gorithm' to drive the \n% 'controlledVariable' to follow the 'setPoint'  \n% The manipulated variable is limited"
      " between 'mvMax' and 'mvMin'\n% The controller proportional, integral and derivative gains are Kc, Ti, Td\n\n% D"
      "ate: 1 Oct 2010\n% Author: JC Peyton Jones\n% Villanova University\n\n% Initialize persistent variables\npersist"
      "ent ekm1 ekm2 mvm1\n\nif (isempty(ekm1)),\n    ekm1= 0;        % e(k-1) is the error at time k-1\n    ekm2= 0;  "
      "      % e(k-2) is the error at time k-2\n    mvm1= 0;        % mv(k-1) is the manipulated variable at k-1\nend\n"
      "\n% Compute the control action (the manipulated variable)\nek= setPoint - contolledVariable;\ndm = Kc*(ek-ekm1 +"
      " (1/Ti)*ek + Td*(ek - 2*ekm1 + ekm2));\nmv = mvm1 + dm;\n\n% Limit the control action\nif (mv > mvMax),\n    mv "
      "= mvMax;\nelseif (mv < mvMin),\n    mv = mvMin;\nend;\n\n% Shift variables ready for next time\nekm2= ekm1;     "
      "% e(k-2)= e(k-1);\nekm1= ek;       % e(k-1)= e(k);\nmvm1= mv;       % mv(k-1)= mv;\n\n\n"
      editorLayout	      "100 M4x1[145 145 945 623]"
      fimathString	      "fimath(...\n'RoundMode', 'floor',...\n'OverflowMode', 'wrap',...\n'ProductMode', 'KeepLSB', "
      "'ProductWordLength', 32,...\n'SumMode', 'KeepLSB', 'SumWordLength', 32,...\n'CastBeforeSum', true)"
      fimathForFiConstructors FimathMatlabFactoryDefault
      emlDefaultFimath	      FimathUserSpecified
    }
  }
