      "      % e(k-2) is the error at time k-2\n    mvm1= 0;        % mv(k-1) is the manipulated variable at k-1\nend\n"
      "\n% Compute the control action (the manipulated variable)\nek= setPoint - contolledVariable;\ndm = Kc*(ek-ekm1 +"
      " (1/Ti)*ek + Td*(ek - 2*ekm1 + ekm2));\nmv = mvm1 + dm;\n\n% Limit the control action\nif (mv > mvMax),\n    mv "
      "= mvMax;\nelseif (mv < mvMin),\n    mv = mvMin;\nend;\n\n% Shift variables ready for next time\nekm2= ekm1;     "
